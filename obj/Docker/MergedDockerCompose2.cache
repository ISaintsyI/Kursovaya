name: dockercompose7349585104668239068
services:
  api:
    build:
      context: D:\Projects\C# Projects\Nomerogram
      dockerfile: Api/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: api
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
  postgres:
    environment:
      PGDATA: /var/lib/postgresql/data/pgdata
      POSTGRES_PASSWORD: root
      POSTGRES_USER: root
    image: postgres:15
    networks:
      network: null
    ports:
    - mode: ingress
      target: 5432
      published: "5432"
      protocol: tcp
    volumes:
    - type: volume
      source: postgres-data
      target: /var/lib/postgresql/data
      volume: {}
networks:
  default:
    name: dockercompose7349585104668239068_default
  network:
    name: dockercompose7349585104668239068_network
    driver: bridge
volumes:
  postgres-data:
    name: dockercompose7349585104668239068_postgres-data